-- game handling

function handle_common()
	if btnp(🅾️) then
		_upd=update_menu
		_drw=draw_menu
	end
end

function handle_tetris()
	chip8:set_key(5,btn(⬅️))
	chip8:set_key(6,btn(➡️))
	chip8:set_key(7,btn(⬇️))
	chip8:set_key(4,btn(❎))
	handle_common()
end

function handle_cave()
	chip8:set_key(4,btn(⬅️))
	chip8:set_key(6,btn(➡️))
	chip8:set_key(8,btn(⬇️))
	chip8:set_key(2,btn(⬆️))
	chip8:set_key(0xf,btnp(❎))
	handle_common()
end

