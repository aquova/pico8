function prints(_t,_x,_y,_c1,_c2)
	print(_t,_x+1,_y+1,_c2)
	print(_t,_x,_y,_c1)
end

function printpc(_t,_y,_c)
	local w=8*#_t-2
	local x=(128-w)/2
	print("\^p".._t,x,_y,_c)
end
