-- drawing

function draw_base()
	cls()
	draw_bg()
	draw_board()
	draw_tiles()
	draw_pts()
	draw_time()
	draw_rots()
	draw_highlight()
	shake_screen()
end

function draw_game()
	draw_base()
	draw_floating()
end

function draw_click()
	draw_base()
end

function draw_board()
	local x1,y1=board_margin,board_margin
	local x2=scrn-board_margin-1
	local y2=scrn-board_margin-1

	rectfill(scrn/2-1,y1-board_tab-1,x2+1,y2+1,6)
	rectfill(x1-1,y1-1,scrn/2+1,y2+board_tab+1,6)
	rectfill(scrn/2,y1-board_tab,x2,y2,0)
	rectfill(x1,y1,scrn/2,y2+board_tab,0)
end

function draw_tiles()
	for x=1,board_size do
		for y=1,board_size do
			local sprt=grid[x][y]
			local gx=board_margin+tile_size*(x-1)+1
			local gy=board_margin+tile_size*(y-1)
			sprt:draw(gx,gy)
		end
	end
end

function draw_pts()
	local x=66
	local y=board_margin-7

	local pts_t=""..pts
	local pts_len=#pts_t
	for i=0,pts_digits-pts_len do
		pts_t="0"..pts_t
	end
	print(pts_t,x,y,7)
end

function draw_time()
	local x=91
	local y=board_margin-7
	local dt=max_time-(time()-start_t)
	local t=format_t(max(0,dt))
	local c=7
	if dt<10 then
		c=8
	end
	print(t,x,y,c)
end

function draw_highlight()
	local cx,cy=get_cel(g_cursor.x,g_cursor.y)
	rect(cx,cy,cx+tile_size-1,cy+tile_size-1,7)
end

function draw_floating()
	for f in all(floating) do
		f:draw()
	end
end

function draw_rots()
	local x=board_margin+1
	local y=scrn-board_margin+1
	for i=1,rots do
		spr(12,x+(i-1)*10,y)
	end
end
