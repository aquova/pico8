-- on click event

function on_click()
	-- todo: fix these being global
	gx=g_cursor.x+1
 gy=g_cursor.y+1
 local t=grid[gx][gy]
	d=t:get_dir()
	t:set_highlight(true)
 c=t:get_color()
 streak=1

 local cx,cy=get_cel(g_cursor.x,g_cursor.y)
 np+=match_pts
 add(floating,make_label(match_pts,cx,cy))

 _upd=update_click
 _drw=draw_click
end

function update_click()
	if frames%2==0 then
		return
	end

	sfx(3)
	if d==dirs.up then
		gy-=1
	elseif d==dirs.down then
		gy+=1
	elseif d==dirs.left then
		gx-=1
	elseif d==dirs.right then
		gx+=1
	end

	-- if we hit a wall, break
	if gx~=mid(1,gx,board_size) then
		fall_tiles()
		return
	end
	if gy~=mid(1,gy,board_size) then
	 fall_tiles()
	 return
	end

	local nt=grid[gx][gy]
	-- if we find our own color, use it
	if c==nt:get_color() then
		-- if we hit a previous tile, break
		if nt:is_highlighted() then
			fall_tiles()
			return
		end

		nt:set_highlight(true)
		d=nt:get_dir()
		streak+=1
		np+=streak*match_pts
		
		local cx,cy=get_cel(gx-1,gy-1)
		add(floating,make_label(streak*match_pts,cx,cy))
	else
		nt:set_checked(true)
		np+=1

		local cx,cy=get_cel(gx-1,gy-1)
		add(floating,make_label(1,cx,cy))
	end
end

function fall_tiles()
	_upd=update_fall
	shake+=1
end

function update_fall()
	if streak==1 then
		floating={}
		reset_tiles()
		sfx(4)
	else
		sfx(2)
		pts+=np
		update_floating()

		for x=1,board_size do
			local empty={}
			for y=board_size,1,-1 do
				local t=grid[x][y]
				t:set_checked(false)
				t:reset_rotated()

				if t:is_highlighted() then
					-- remove tile
					add(empty,y)
					explosion(
						board_margin+tile_size*(x-1)+tile_size/2,
						board_margin+tile_size*(y-1)+tile_size/2
					)
				elseif #empty~=0 then
					-- move tile down
					local y2=empty[1]
					grid[x][y],grid[x][y2]=grid[x][y2],grid[x][y]
					del(empty,y2)
					add(empty,y)
				end
			end
			-- repopulate empty tiles at top
			for i in all(empty) do
				grid[x][i]=rnd_tile()
			end
		end
	end

	rots=max_rot
	np=0
	streak=0
	_upd=update_game
	_drw=draw_game
end

