-- post-game

default_hs={
	1000,750,500,300,200,
	150,100,50,25,10
}

function update_gameover()
	if btnp(‚ùé) then
		_upd=update_hs
		_drw=draw_hs
		sfx(1)
	end

	update_floating()
end

function draw_gameover()
	draw_game()
	printb("time over",48,60,8,0)
end

function update_hs()
	if btnp(‚ùé) then
		_upd=update_title
		_drw=draw_title
		music(0)
	end
end

function draw_hs()
	cls()
	draw_bg()
	printb("high scores",42,5,6,0)

	local x=48
	local y=10
	local dy=10
	for i=1,#default_hs do
		local n=i..". "
		if i<10 then
			n=n.." "
		end
		printb(n..hs[i],x,y+dy*i,6,0)
	end
end

function load_hs()
	if dget(0)==0 then
		reset_hs()
		return default_hs
	else
		scores={}

		for i=1,#default_hs do
			add(scores,dget(i))
		end

		return scores
	end
end

function reset_hs()
	save_hs(default_hs)
	dset(0,1)
end

function save_hs(_tbl)
	for i=1,#_tbl do
		dset(i,_tbl[i])
	end
end

function add_score(_tbl,_score)
	local hs_found=false
	local tmp=nil
	for i=1,#_tbl do
		local s=_tbl[i]
		if _score>s and not hs_found then
			hs_found=true
			tmp=s
			_tbl[i]=_score
		elseif hs_found then
			_tbl[i]=tmp
			tmp=s
		end
	end

	if hs_found then
		save_hs(_tbl)
	end
end
