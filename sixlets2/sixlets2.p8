-- title screen

function update_intro()
	if btnp(‚ùé) or frames-scrn>=logo_y then
		_upd=update_title
		_drw=draw_title
		music(0)
	end
end

function draw_intro()
	cls()
	draw_bg()
	local y=min(frames-scrn,16)
	draw_logo(y)
end

function update_title()
	if btnp(‚ùé) then
		reset_game()
		music(-1,500)
		sfx(1)
		_upd=update_game
		_drw=draw_game
	end
end

function draw_title()
	cls()
	draw_bg()
	draw_logo(logo_y)
	print("press ‚ùé to start",32,100,flash())
	printb("aquova",100,120,0,1)
end

function reset_game()
	shake=0
	floating={}

	grid=populate_grid()
	g_cursor={x=0,y=0}
	rots=max_rot

	np=0
	pts=0
	streak=0
	hs=load_hs()
	start_t=time()
end

function init_stars()
	for i=1,128 do
		add(stars,{x=rnd(40)-20,y=rnd(40)-20,z=-rnd(30)})
	end
end

-- star effect from https://www.lexaloffle.com/bbs/?pid=62162#p
function draw_bg()
	for s in all(stars) do
		local x1,y1=project(s.x,s.y,s.z)
		local x2,y2=project(s.x,s.y,mid(-100,s.z+star_spd,-0.1))
		line(x1,y1,x2,y2,7)

		s.z+=star_spd/10

		if s.z>-0.1 then
			s.x=rnd(40)-20
			s.y=rnd(40)-20
			s.z=-30
		end
	end

	if star_spd<5 then
		star_spd+=0.05
	end
end

function project(x,y,z)
	return flr((-x/z)*64+64),flr((-y/z)*64+64)
end

function draw_logo(_y)
	sspr(0,16,94,51,16,_y)
	sspr(94,16,22,25,52,_y+52)
end
