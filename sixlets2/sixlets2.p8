-- floating effects

function update_floating()
	for f in all(floating) do
		f:update()
		if f:should_die() then
			del(floating,f)
		end
	end
end

function explosion(_x,_y)
	for _=1,num_particles do
		local p=make_particle(_x,_y)
		add(floating,p)
	end
end

function make_particle(_x,_y)
	local p={
		x=_x,y=_y,age=0,a=rnd(),
		col=rnd({8,9,3,12,13,6}),
		spd=1+rnd(2),
		max_age=15+rnd(5)
	}

	p.dx=sin(p.a)*p.spd
	p.dy=cos(p.a)*p.spd

	function p:update()
		self.x+=self.dx
		self.y+=self.dy
		self.age+=1
		-- gravity
		self.dy+=0.15
	end

	function p:should_die()
		return self.age>self.max_age
	end

	function p:draw()
		line(
			self.x,self.y,
			self.x+self.dx,self.y+self.dy,
			self.col
		)
	end

	return p
end

function make_label(_pt,_x,_y)
	local l={
		text="+".._pt,age=0,max_age=20
	}

	l.x=_x+4-#l.text/2
	l.y=_y-5

	function l:update()
		self.y-=1
		self.age+=1
	end

	function l:should_die()
		return self.age>self.max_age
	end

	function l:draw()
		printb(self.text,self.x,self.y,7,0)
	end

	return l
end

