-- utils

function get_cel(gx,gy)
	local cx=board_margin+gx*tile_size
	local cy=board_margin+gy*tile_size
	return cx,cy
end

function printb(_t,_x,_y,_ci,_co)
	for dx=-1,1 do
		for dy=-1,1 do
			print(_t,_x+dx,_y+dy,_co)
		end
	end

	print(_t,_x,_y,_ci)
end

function flash()
	local f_tbl={2,8,14,8}
	local idx=(frames\4)%#f_tbl
	return f_tbl[idx+1]
end

function shake_screen()
	local sx=(1-rnd(2))*shake
	local sy=(1-rnd(2))*shake

	camera(sx,sy)

	shake*=0.75
	if shake<0.1 then
		shake=0
	end
end

function format_t(_t)
	local m=_t\60
	local s=flr(_t%60)
	if s<10 then
		s="0"..s
	end

	return "0"..m..":"..s
end
