-- constants
scrn=128
tile_size=12
board_margin=16
board_size=(scrn-2*board_margin)/tile_size
board_tab=10
num_colors=6
logo_y=16
num_particles=50
max_rot=5
max_time=90
pts_digits=4
match_pts=3

dirs={
	up=1,
	right=2,
	down=3,
	left=4,
}

function _init()
	cartdata("aquova_sixlets2_v2")

	frames=0
	star_spd=0
	stars={}
	init_stars()

	_upd=update_intro
	_drw=draw_intro
	music(4)
end

function _update()
	frames+=1
	_upd()
end

function _draw()
	_drw()
end
