-- main game logic

function update_game()
	if is_timeover() then
		add_score(hs,pts)
		_upd=update_gameover
		_drw=draw_gameover
	end

	update_floating()

	if btnp(‚¨ÖÔ∏è) then
		g_cursor.x=(g_cursor.x-1)%board_size
		sfx(0)
	elseif btnp(‚û°Ô∏è) then
		g_cursor.x=(g_cursor.x+1)%board_size
		sfx(0)
	end

	if btnp(‚¨ÜÔ∏è) then
		g_cursor.y=(g_cursor.y-1)%board_size
		sfx(0)
	elseif btnp(‚¨áÔ∏è) then
		g_cursor.y=(g_cursor.y+1)%board_size
		sfx(0)
	end

	if btnp(üÖæÔ∏è) then
		rotate_tile()
	end

	if btnp(‚ùé) then
		on_click()
	end
	
	local dt=max_time-(time()-start_t)
	if dt<10 then
		if time()%1<0.05 then
			sfx(5)
		end
	end
end

function is_timeover()
	return time()-start_t>max_time
end

function populate_grid()
	local g={}

	for x=0,board_size-1 do
		local row={}
		for y=0,board_size-1 do
			local t=rnd_tile()
			add(row,t)
		end
		add(g,row)
	end

	return g
end

function rnd_tile()
	local t={}

	t.color=ceil(rnd(num_colors))
	t.dir=ceil(rnd(4))
	t.start_dir=t.dir
	t.highlight=false
	t.checked=false

	function t:get_color()
		return self.color
	end

	function t:reset_rotated()
		self.start_dir=self.dir
	end

	function t:set_highlight(_h)
		self.highlight=_h
	end

	function t:set_checked(_c)
		self.checked=_c
	end

	function t:is_highlighted()
		return self.highlight
	end

	function t:is_checked()
		return self.checked
	end

	function t:is_rotated()
		return self.dir~=self.start_dir
	end

	function t:get_dir()
		return self.dir
	end

	function t:rotate()
		t.dir=(t.dir%4)+1
	end

	function t:draw(_x,_y)
		if self:is_highlighted() then
			rectfill(
				_x,_y,
				_x+10,_y+10,
				7
			)
		end

		if self:is_checked() then
			rectfill(
				_x,_y,
				_x+10,_y+10,
				4
			)
		end

		local sprt_color=self.color-1
		sspr(
			sprt_color*tile_size+8,0,
			tile_size,tile_size,
			_x,_y
		)

		local arrow_spr=11
		local flip_x=false
		local flip_y=false
		local ax=_x+2
		local ay=_y+2
		if self.dir==dirs.down then
			flip_y=true
		elseif self.dir==dirs.left then
			arrow_spr=10
			flip_x=true
			ax-=1
		elseif self.dir==dirs.right then
			arrow_spr=10
		end

		spr(arrow_spr,
			ax,ay,
			1,1,
			flip_x,flip_y
		)
	end

	return t
end

function rotate_tile()
	local t=grid[g_cursor.x+1][g_cursor.y+1]
	if t:is_rotated() then
		t:rotate()
		if not t:is_rotated() then
			rots+=1
		end
	elseif rots~=0 then
		t:rotate()
		rots-=1
	end
end

function reset_tiles()
	for x=1,board_size do
		for y=1,board_size do
			local t=grid[x][y]
			t:set_highlight(false)
			t:set_checked(false)
			t:reset_rotated()
		end
	end
end
